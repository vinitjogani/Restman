<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<VBox fx:id="cellContainer" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.121"
      xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox prefWidth="269.0">
            <children>
                <Label fx:id="foodItemLabel" maxWidth="1.7976931348623157E308" prefHeight="21.0" prefWidth="73.0"
                       text="MenuItem" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets right="10.0"/>
                    </HBox.margin>
                </Label>
                <Label fx:id="orderId" prefHeight="21.0" prefWidth="73.0" text="Order #"/>
            </children>
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
        </HBox>
        <ListView fx:id="ingredientList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                  mouseTransparent="true"/>
        <HBox prefWidth="200.0" fx:id="notesBox">
            <children>
                <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                        onAction="#getNotes" text="Notes" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
</VBox>
